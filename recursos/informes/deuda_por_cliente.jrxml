	<queryString language="SQL">
		<![CDATA[SELECT cliente.cedula,
	cliente.nombre || ' ' || cliente.apellido as cliente,
	cuenta_cliente.direccion,
	cliente.telefono,
	(planilla.total_pagar - pagos.valor) as deuda,
	planilla.id_planilla,
	planilla.fecha, medidor.codigo,
	$P{usuario_crea} as usuarioCrea,
	$P{fecha_impresion} as fechaImpresion
FROM cliente,
	cuenta_cliente,
	medidor,
	planilla,
	pagos
WHERE 
	cliente.id_cliente = cuenta_cliente.id_cliente and
	cuenta_cliente.id_cuenta = planilla.id_cuenta and
	planilla.id_planilla = pagos.id_planilla and
	medidor.id_medidor = cuenta_cliente.id_medidor and
	planilla.cancelado = 'PENDIENTE' and
	(cliente.cedula = $P{patron} or cliente.nombre = $P{patron} or
	cliente.apellido = $P{patron} )]]>
	</queryString>
