	<queryString language="SQL">
		<![CDATA[SELECT (c.nombre || ' ' ||c.apellido) as nombres,
	c.id_cliente,
	i.referencia,
	i.fecha,
	i.id_solicitud_rep
FROM sol_inspeccion_rep i,
	cliente c,
	cuenta_cliente cc
WHERE 
