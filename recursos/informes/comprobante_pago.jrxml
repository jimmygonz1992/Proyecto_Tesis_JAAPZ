	<parameter name="numComprobante" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT cliente.cedula, cliente.nombre, cliente.apellido,
	factura.fecha, factura.num_factura, factura.total_factura, factura.usuario_crea,
	factura_detalle.id_detalle_fac, planilla_detalle.cantidad, planilla_detalle.descripcion,
	planilla_detalle.subtotal, planilla.id_planilla, factura_detalle.id_factura, factura_detalle.subtotal as subtFac,
	anio.descripcion as anio, mes.descripcion as mes, planilla.total_pagar
FROM cliente,
	cuenta_cliente,
	medidor,
	factura,
	factura_detalle,
	planilla,
	planilla_detalle,
	pagos,
	mes,
	apertura_lectura,
	anio
WHERE 
	 cliente.id_cliente = cuenta_cliente.id_cliente 
	 AND cuenta_cliente.id_medidor = medidor.id_medidor 
	 AND cuenta_cliente.id_cuenta = factura.id_cuenta
	 AND cuenta_cliente.id_cuenta = planilla.id_cuenta
	 AND factura.id_factura = factura_detalle.id_factura 
	 AND factura_detalle.id_planilla = planilla.id_planilla 
	 AND planilla.id_planilla = planilla_detalle.id_planilla 
	 AND planilla.id_planilla = pagos.id_planilla 
	 AND planilla.id_apertura = apertura_lectura.id_apertura 
	 AND apertura_lectura.id_mes = mes.id_mes 
	 AND anio.id_anio = apertura_lectura.id_anio 
	 AND factura.num_factura =  $P{numComprobante}]]>
	</queryString>
